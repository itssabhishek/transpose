{
  "name": "Transpose",
  "description": "Divided by Streaming Platforms United by Our Love For Music",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "action": {
    "default_title": "Default Title",
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png",
      "512": "icons/512.png"
    }
  }
}
  